{
  "name": "find-nearest-tailwind-colour",
  "description": "Find the nearest colour from Tailwind's colour palette",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "watch:css": "postcss src/assets/global.pcss -o static/global.css -w",
    "watch:dev": "sapper dev",
    "dev": "run-p watch:*",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.pcss -o static/global.css",
    "build:sapper": "sapper build",
    "build:export": "sapper export",
    "export": "run-s build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "compression": "^1.7.4",
    "nearest-color": "^0.4.4",
    "polka": "next",
    "sirv": "^1.0.18",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.1.0",
    "rollup": "^2.35.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.31.1",
    "svelte-persistent-store": "0.1.6",
    "svelte-preprocess": "^4.9.8"
  }
}